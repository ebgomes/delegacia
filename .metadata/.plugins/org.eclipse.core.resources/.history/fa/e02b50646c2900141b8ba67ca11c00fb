package br.com.rondofrio.model;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

import lombok.Getter;
import lombok.Setter;
import br.com.rondofrio.arquitetura.AbstractEntity;
import br.com.rondofrio.arquitetura.Identifiable;

@Entity
@Getter
@Setter
public class TipoDeMovimento extends AbstractEntity implements Identifiable{

	private static final long serialVersionUID = 1L;
	
	@Id
	@GeneratedValue
	private Long id;
	
	private String descricao;

	@Override
	public Long getId() {
		return id;
	}
	
	public void setCodigo(Long codigo) {
        setId(codigo);
    }

    public Long getCodigo() {
        return getId();
    }

}
